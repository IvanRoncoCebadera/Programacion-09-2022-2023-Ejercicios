<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="162.0" prefWidth="299.0" spacing="20.0"
      xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="dev.sbytmacke.thechinos.controllers.AcercaDeViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <BorderPane prefHeight="30.0" prefWidth="259.0">
        <left>
            <Label fx:id="myDataLabel" text="Info developer:" BorderPane.alignment="CENTER"/>
        </left>
        <right>
            <ImageView fx:id="infoImage" fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true"
                       BorderPane.alignment="CENTER"/>
        </right>
    </BorderPane>
    <GridPane prefHeight="66.0" prefWidth="259.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="109.0" minWidth="10.0" prefWidth="75.00000762939453"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="156.99999237060547" minWidth="10.0"
                               prefWidth="156.99999237060547"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <children>
            <Label fx:id="myNameLabel" text="Nombre:"/>
            <Label fx:id="myGithubLabel" text="Github:" GridPane.rowIndex="1"/>
            <Hyperlink fx:id="myGithubLink" text="Sbytmacke" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <Label fx:id="myNameDataLabel" text=" Angel Maroto Chivite" GridPane.columnIndex="1"/>
        </children>
    </GridPane>
</VBox>
